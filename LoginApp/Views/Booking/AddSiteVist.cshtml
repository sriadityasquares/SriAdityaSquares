            //$("#ImmediateSeniorID").append("<option value='" + 0 + "'>" + "--Select Sr. Agent--" + "</option>")
            $.each(data, function (index, row) {
                $("#ImmediateSeniorID").append("<option value='" + row.AgentID + "'>" + row.AgentName + "</option>")
